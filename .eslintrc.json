{
    "plugins": [
      "jest"
    ],
    "env": {
        "node": true,
        "jest": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
     "no-console": "off",
     "import/extensions": "off",
     "no-underscore-dangle": [1, { "allow": ["__dirname"] }]
    }
}
